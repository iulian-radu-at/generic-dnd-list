{
  "name": "generic-dnd-list",
  "version": "2.2.0",
  "description": "A DnD list with a custom renderer for its items.",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production yarn build && yarn publish --non-interactive",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook:start": "start-storybook -c storybook",
    "storybook:build": "build-storybook -c storybook -o docs",
    "storybook:commit": "git add . && git commit -m \"Updated storybook\"",
    "all": "yarn install && yarn storybook:build && yarn deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/generic-dnd-list.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "img",
    "dnd list",
    "generic-dnd-list",
    "react",
    "react-component",
    "ui"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/generic-dnd-list/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/generic-dnd-list#readme",
  "dependencies": {
    "react-beautiful-dnd": "13.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.0",
    "@storybook/react": "6.0.28",
    "@types/babel-core": "6.25.7",
    "@types/enzyme": "3.10.11",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/node": "17.0.16",
    "@types/react": "16.9.56",
    "@types/react-beautiful-dnd": "13.1.2",
    "@types/react-dom": "16.9.11",
    "@types/rimraf": "3.0.2",
    "babel-core": "6.26.3",
    "babel-jest": "27.5.1",
    "babel-loader": "8.2.3",
    "babel-preset-react-app": "10.0.1",
    "cross-env": "7.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "rimraf": "3.0.2",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": ">= 16.9.0",
    "react-dom": ">= 16.9.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "transform": {
      ".+\\.js$": "babel-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
