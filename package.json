{
  "name": "generic-dnd-list",
  "version": "3.0.0",
  "description": "A DnD list with a custom renderer for its items.",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production yarn build && yarn publish --non-interactive",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook:start": "start-storybook -c storybook",
    "storybook:build": "build-storybook -c storybook -o docs",
    "storybook:commit": "git add . && git commit -m \"Updated storybook\"",
    "all": "yarn reinstall && yarn storybook:build && yarn deploy",
    "reinstall": "rimraf yarn.lock node_modules && yarn install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/generic-dnd-list.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "img",
    "dnd list",
    "generic-dnd-list",
    "react",
    "react-component",
    "ui"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/generic-dnd-list/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/generic-dnd-list#readme",
  "dependencies": {
    "react-beautiful-dnd": "^13.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "6.5.14",
    "@types/babel-core": "6.25.7",
    "@types/node": "18.11.13",
    "@types/react": "^18.0.26",
    "@types/react-beautiful-dnd": "^13.1.3",
    "@types/react-dom": "^18.0.9",
    "@types/rimraf": "3.0.2",
    "babel-core": "6.26.3",
    "babel-jest": "29.3.1",
    "babel-loader": "9.1.0",
    "babel-preset-react-app": "10.0.1",
    "cross-env": "7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "3.0.2",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": ">= 18.0.0",
    "react-dom": ">= 18.0.0"
  }
}
